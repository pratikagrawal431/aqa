.select-style {
    font-size: 14px;
    color: #999999;  
	padding: 11px 10px;
	font-weight:700;
	width:100%;
	margin-top:14px;
	margin-bottom:10px;
	border:2px solid #cccccc;
    width:100%;
    overflow: hidden;
    background-color: #fff;
    background: url("images/arrowdown.jpg") no-repeat 95% 50%;
}

.select-style select {
   /*padding:10px 0 10px 3px;*/
	color:#999999;
font-family:'Lato';
font-size:14px;
    width: 105%;
    border: none;
    box-shadow: none;
    background-color: transparent;
    background-image: none;
    -webkit-appearance: none;
       -moz-appearance: none;
            appearance: none;
}

.select-style select:focus {
    outline: none;
}

.nav-tabs > li > a:hover
{
    background: none!important;
    border: none!important;
}

.input-error {
    width: 100%;
    padding: 8px 10px;
    margin-bottom: 5px;
    border: 1px solid #d9132d !important ;
    font-family: 'Montserrat-Light';
    font-size: 14px;
    color: #656565;
}
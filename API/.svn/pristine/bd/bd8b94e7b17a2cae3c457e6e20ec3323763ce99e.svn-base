.get-rates { background: #00995e; border-bottom: 3px solid #cccccc; margin-top:30px; margin-bottom: 20px; border-left: 1px solid #bdbdbd; border-right: 1px solid #bdbdbd; border-top: 1px solid #bdbdbd; text-align: center; color: #fff; font-size: 16px; font-weight: 700; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; padding: 13px 25px; border: none; }

.submit { background: #00995e; border-bottom: 3px solid #cccccc; margin-top:30px; margin-bottom: 20px; border-left: 1px solid #bdbdbd; border-right: 1px solid #bdbdbd; border-top: 1px solid #bdbdbd; text-align: center; color: #fff; font-size: 16px; font-weight: 700; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; padding: 13px 30px; border: none; }

*:focus {
    outline: none;
}




body
{
    font-family: 'Lato', sans-serif!important;
}

.terms-use li { list-style:decimal; word-wrap: break-word;}
.terms-use-ul li { list-style:lower-alpha;}
.privacy { color: #00ad6a; font-weight: 700; text-align: left; font-size: 16px; line-height: 40px;}
.privacy-heading { color: #414141; text-align: left; font-size: 13px; line-height: 22px;}
.privacy-heading-li a{color: #00ad6a; text-align: left; font-size: 13px; line-height: 22px;}
.privacy-heading-li { color: #414141; text-align: left; font-size: 13px; line-height: 20px; margin-bottom: 20px; }
.awesome-font { color: #00995e; font-weight: 300; text-align: left; font-size: 24px; padding: 30px 0 0px 0px; line-height: 40px; }
.heading-left { color: #414141; font-weight: 300; text-align: left; font-size: 24px; padding: 30px 0 0px 0px; line-height: 40px; }
.heading { color: #414141; font-weight: 300; text-align: center; font-size: 24px; padding: 30px 0 50px 0px; line-height: 40px; }
.heading2 { color: #414141; font-weight: 300; text-align: center; font-size: 14px; padding: 30px 0 50px 0px; line-height: 22px; }
.heading3 { color: #464646; font-weight: 400; text-align: center; font-size: 13px; padding: 30px 0 50px 0px; line-height: 20px; }
.heading3-left { color: #464646; font-weight: 400; text-align: left; font-size: 14px; padding: 0px 0 20px 0px; line-height: 22px; }

.click-button { background: #fff; width: 100%; border-bottom: 3px solid #cccccc; margin-bottom: 10px; border-left: 1px solid #bdbdbd; border-right: 1px solid #bdbdbd; border-top: 1px solid #bdbdbd; text-align: center; color: #464646; font-size: 14.5px; font-weight: 700; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; border-radius: 5px; padding: 15px 0px; }

.click-button:hover { background: #00ad6a; color: #fff; border-bottom: 3px solid #00995e; border-left: 1px solid #00ad6a; border-right: 1px solid #00ad6a; border-top: 1px solid #00ad6a; }

.click-buttonRed { background: #ac172b; width: 100%; border: none; text-align: center; color: #fff; font-size: 14px; font-weight: 700; -webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; padding: 15px 0px; }

input 
{ 
box-shadow: inset 0px 1px 6px 0px #ABABAB,0px 0px 0px 0px #DDDDDD;
-webkit-box-shadow: inset 0px 1px 6px 0px #ABABAB,0px 0px 0px 0px #DDDDDD;
-moz-box-shadow: inset 0px 1px 6px 0px #ABABAB,0px 0px 0px 0px #DDDDDD;
-o-box-shadow: inset 0px 1px 6px 0px #ABABAB,0px 0px 0px 0px #DDDDDD;
    
    margin: 10px 0;
    padding: 10px 15px;
    width: 100%;
    font-size: 14px;
    color: #696969;
    font-weight: 700;
}


.left-inner-addon {
    position: relative;
}
.left-inner-addon input {
    padding-left: 24px;    
}
.left-inner-addon span {
    position: absolute;
    padding: 22px 13px;
    pointer-events: none;
    font-size: 14px;
    color: #999999;
}

.purchase-addon {
    position: relative;
}
.purchase-addon input {
    padding-left: 20px;    
}
.purchase-addon span {
    color: #999999;
    font-size: 14px;
    padding: 24px 0;
    pointer-events: none;
    position: absolute;
    right: 22px;
}


.link-Goback
{
    color: #00995e;
}
.link-Goback:hover
{
    color: #00995e;
    text-decoration: none;
}
.href
{
    text-decoration: none;
     border-bottom: 1px dotted #000;
    color: #414141; font-weight: 300;
}

.href:hover
{
    text-decoration: none;
}

.purchase-link
{
   font-size: 24px;
    font-weight: 700;
    color: #00995e; 
}

.list-group-item-Link {
    position: relative;
    display: block;
    padding: 12px 15px;
    margin-bottom: -1px;
    background:none!important;
    border-bottom: 1px solid #d7dbd0;
    
    font-size: 18px;
    font-weight: 700!important;
    color: #00995e!important; 
}

.list-group-item-Link:hover
{
 background:#00995e!important;
 color: #fff!important; 
}
.list-group-item-Link:hover .arrow-right
{
 color: #fff!important; 
}


.list-group-item>.arrow-right {
    float: right;
}

.arrow-right {
    display: inline-block;
    min-width: 10px;
    padding: 0px 7px;
    font-size: 22px;
    font-weight: 700;
    line-height: 1;
    color: #00995e;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
}

.container {
    padding-right: 13px;
    padding-left: 13px;
    margin-right: auto;
    margin-left: auto;
}


.nav-tabs > li {
    float: left;
    margin-bottom: -1px;
    text-align: center;
    width: 33.3%;
}
.nav>li>a {
    position: relative;
    display: block;
    padding: 10px 0px;
    color: #323232; 
}
.nav-tabs a
{
font-weight: 700!important;
      
	font-size: 15px;
	margin-top:30px;
}

.nav-tabs > .active > a, .nav-tabs > .active > a:focus, .nav-tabs > .active > a:hover
{
    border-bottom-color: #00995e!important;
	border-top-color: #fff!important;
	border-left-color: #fff!important;
	border-right-color: #fff!important;
    color: #00995e!important;
    border-bottom: 3px solid!important;
}

.input-title
{
 font-size: 16px;
    font-weight: 700;
	color: #323232;
	margin-top:10px;
}
.input-title2
{
 font-size: 16px;
    font-weight: 700;
	color: #323232;
	margin-top:20px;
}

.input-down-payment
